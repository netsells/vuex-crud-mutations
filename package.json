{
    "name": "@netsells/vuex-crud-mutations",
    "version": "1.1.2",
    "description": "Easily scaffold Vuex CRUD mutations",
    "homepage": "http://github.com/netsells/vuex-crud-mutations",
    "author": {
        "name": "Sam Turrell",
        "email": "sam.turrell@netsells.co.uk",
        "url": "http://samturrell.co.uk"
    },
    "files": [
        "src/",
        "dist/"
    ],
    "main": "dist/index.js",
    "keywords": [
        "vue",
        "vuex",
        "javascript",
        "mutations",
        "crud"
    ],
    "devDependencies": {
        "all-contributors-cli": "^5.4.0",
        "babel-cli": "^6.5.1",
        "babel-core": "^6.26.3",
        "babel-eslint": "^6.0.0",
        "babel-jest": "^23.4.2",
        "babel-plugin-transform-function-bind": "^6.5.2",
        "babel-preset-es2015": "^6.5.0",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-config-xo": "^0.20.1",
        "eslint-plugin-prettier": "^2.6.0",
        "husky": "^2.4.0",
        "jest": "^22.0.6",
        "lint-staged": "^6.1.1",
        "nsp": "^2.8.0",
        "prettier": "^1.11.1",
        "regenerator-runtime": "^0.12.1",
        "vue": "^2.5.17",
        "vuex": "^3.0.1",
        "@netsells/eslint-config": "^1.3.0"
    },
    "engines": {
        "npm": ">= 4.0.0"
    },
    "scripts": {
        "build": "babel -d ./dist ./src",
        "prepublishOnly": "nsp check",
        "pretest": "eslint .",
        "precommit": "lint-staged",
        "test": "jest tests/ --testRegex=.*\\.test\\.js$",
        "contributors:add": "all-contributors add",
        "contributors:check": "all-contributors check",
        "contributors:generate": "all-contributors generate"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": [
            "xo",
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true
        },
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "singleQuote": true,
                    "printWidth": 90
                }
            ]
        },
        "plugins": [
            "prettier"
        ]
    },
    "repository": "git@github.com:netsells/vuex-crud-mutations.git",
    "jest": {
        "testEnvironment": "node",
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    },
    "license": "MIT"
}
