{
    "name": "vuex-crud-mutations",
    "version": "0.0.0",
    "description": "Easily scaffold Vuex CRUD mutations",
    "homepage": "",
    "author": {
        "name": "Sam Turrell",
        "email": "sam.turrell@netsells.co.uk",
        "url": ""
    },
    "files": [
        "lib"
    ],
    "main": "lib/index.js",
    "keywords": [
        "vue",
        "vuex",
        "javascript",
        "mutations",
        "crud"
    ],
    "devDependencies": {
        "all-contributors-cli": "^5.4.0",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-config-xo": "^0.20.1",
        "eslint-plugin-prettier": "^2.6.0",
        "husky": "^0.14.3",
        "jest": "^22.0.6",
        "lint-staged": "^6.1.1",
        "nsp": "^2.8.0",
        "prettier": "^1.11.1",
        "vue": "^2.5.17",
        "vuex": "^3.0.1"
    },
    "engines": {
        "npm": ">= 4.0.0"
    },
    "scripts": {
        "prepublishOnly": "nsp check",
        "pretest": "eslint .",
        "precommit": "lint-staged",
        "test": "jest --testRegex=.*\\.test\\.js$",
        "contributors:add": "all-contributors add",
        "contributors:check": "all-contributors check",
        "contributors:generate": "all-contributors generate"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": [
            "xo",
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true
        },
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "singleQuote": true,
                    "printWidth": 90
                }
            ]
        },
        "plugins": [
            "prettier"
        ]
    },
    "repository": "git@github.com:netsells/vuex-crud-mutations.git",
    "jest": {
        "testEnvironment": "node"
    },
    "license": "MIT",
    "dependencies": {
        "eslint-config-netsells": "^1.3.0"
    }
}
